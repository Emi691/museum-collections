<h1>Log In</h1>
<br>
<%= form_for @user, url: login_path do |f| %>
    <%= f.label 'username' %>
    <%= f.text_field 'username' %><br>
    <%= f.label 'password' %>
    <%= f.password_field 'password' %><br>
    <br>
    <%= f.submit 'Log In' %>
<% end %>

<%= link_to('Log in with Google', '/auth/google_oauth2') %>